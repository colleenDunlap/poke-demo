{

  "hosting": {
    "public": "dist/poke-demo",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/public/**",
        "destination": "/public.html"
    },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
